{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "PLAYWRIGHT_BROWSERS_PATH": "/opt/vercel/.playwright"
    },
    "functions": {
      "api/**/*.js": {
        "runtime": "vercel-node-18.x"
      }
    },
    "routes": [
      {
        "src": "/.*",
        "dest": "/app.js"
      }
    ]
  }
  